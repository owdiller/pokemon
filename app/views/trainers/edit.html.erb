<h1>Editing Trainer #<%= @trainer.id %></h1>

<form action="/trainers/<%= @trainer.id %>/update">
  <div>
    <label for="name">name</label><br />
    <input id="name" name="name" value="<%= @trainer.name  %>" type="text" />
  </div>
  <div>
    <label for="img">img</label><br />
    <input id="img" name="img" value="<%= @trainer.img  %>" type="text" />
  </div>
  <div>
    <label for="trainer_id">trainer_id</label><br />
    <input id="trainer_id" name="trainer_id" value="<%= @trainer.trainer_id  %>" type="text" />
  </div>
  <div>
    <input type="submit">
  </div>
</form>


